    "formik": "^2.2.9",
    "guid-typescript": "^1.0.9",
    "http-proxy-middleware": "^2.0.4",
    "json2csv": "^5.0.7",
    "lucide-react": "^0.525.0",
    "react": "^19.1.1",
    "react-aria": "^3.36.0",
    "react-chartjs-2": "^5.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^19.1.1",
